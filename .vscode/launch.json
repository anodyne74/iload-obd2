{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch iLoad-OBD2",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["-config", "config.yaml"],
            "env": {},
            "showLog": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "clean-build",
            "postDebugTask": "kill-dlv"
        },
        {
            "name": "Launch TCP Simulator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/testing/cmd/simulator_tcp",
            "args": [],
            "env": {},
            "showLog": true
        },
        {
            "name": "Launch Windows Simulator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/testing/cmd/simulator_windows",
            "args": [],
            "env": {},
            "showLog": true
        },
        {
            "name": "Debug Current File",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
            "env": {},
            "showLog": true
        }
    ]
}